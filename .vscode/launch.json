{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gtest) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/parser_test",
      "args": [
        "--gtest_filter=ParserParseTest.*"
      ],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "MIMode": "lldb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "CMake Test Build"
    },
    {
      "name": "(gdb) Launch (local)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/RT",
      "args": [
        "tests/scenes/full.oadyaml"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "MIMode": "lldb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "make debug"
    },
    {
      "name": "(gdb) Launch client (local)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/RT.client",
      "args": [
        "tests/scenes/full.oadyaml"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "MIMode": "lldb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "make debug"
    },
    {
      "name": "(gdb) Launch (virtualbox)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/RT",
      "args": [
        "maps/map_without_lf"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "DISPLAY",
          "value": "archlinux:10.0"
        }
      ],
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "make debug"
    },
    {
      "name": "(gdb) Launch (WSL)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/RT",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "DISPLAY",
          "value": "127.0.0.1:0.0"
        }
      ],
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "make debug_wsl"
    }
  ]
}
